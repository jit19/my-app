node{
   stage('SCM Checkout'){
     git 'https://github.com/jit19/my-app'
   }
   stage('Compile-Package'){
      // Get maven home path
      def mvnHome =  tool name: 'maven3', type: 'maven'   
      sh "${mvnHome}/bin/mvn package"
   }
   
   stage('SonarQube Analysis') {
        def mvnHome =  tool name: 'maven3', type: 'maven'
        withSonarQubeEnv('SonarQube) { 
          sh "${mvnHome}/bin/mvn sonar:sonar"
        }
    }
   
